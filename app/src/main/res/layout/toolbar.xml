<?xml version="1.0" encoding="utf-8"?><androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="?attr/actionBarSize"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:background="@color/white"    android:id="@+id/toolbar_home"    app:contentInsetStart="0dp"    android:elevation="@dimen/_3sdp"    app:popupTheme="@style/ThemeOverlay.AppCompat"    android:theme="@style/ThemeOverlay.AppCompat.Light">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_gravity="center"        android:layoutDirection="locale"        android:background="@color/white"        android:layout_centerHorizontal="true"        android:focusableInTouchMode="true"        android:orientation="vertical">        <ImageView            android:id="@+id/locationBtn"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:padding="@dimen/_10sdp"            android:layout_gravity="center"            android:layout_alignParentEnd="true"            android:tint="@color/colorPrimary"            android:src="@drawable/ic_baseline_my_location_24"            tools:ignore="UseAppTint" />        <TextView            android:id="@+id/toolbarTitle"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="VA Computing Task"            android:layout_centerVertical="true"            android:layout_marginStart="@dimen/_20sdp"            android:textSize="@dimen/_15sdp"            android:textStyle="bold"            android:textColor="@android:color/black" />    </RelativeLayout></androidx.appcompat.widget.Toolbar>